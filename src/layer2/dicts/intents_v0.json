{
  "intent_layer_version": "v0",
  "intents": [
    {
      "intent_id": "INTENT:base_reduce_shine_target_zones",
      "area": "base",
      "description": "Reduce shine in targeted zones while preserving overall base finish.",
      "trigger_summary": "rule_id indicates excess shine OR lookSpec.base.finish == 'matte' but user base appears dewy; prioritize T-zone control.",
      "expected_outcomes": [
        "less shine in T-zone",
        "base looks smoother",
        "finish stays consistent with LookSpec"
      ]
    },
    {
      "intent_id": "INTENT:base_increase_glow_high_points",
      "area": "base",
      "description": "Increase glow on high points without making the whole face shiny.",
      "trigger_summary": "lookSpec.base.finish == 'dewy' AND rule_id indicates insufficient luminosity; add controlled radiance.",
      "expected_outcomes": [
        "more luminosity",
        "glow concentrated on high points",
        "avoid greasy look"
      ]
    },
    {
      "intent_id": "INTENT:base_thin_layers_for_skinlike",
      "area": "base",
      "description": "Shift base application to thinner layers for a more skin-like finish.",
      "trigger_summary": "lookSpec.base.coverage == 'light' OR rule_id indicates cakey/heavy base; reduce product load.",
      "expected_outcomes": [
        "less cakey appearance",
        "more skin texture realism",
        "even tone retained"
      ]
    },
    {
      "intent_id": "INTENT:base_build_coverage_spot",
      "area": "base",
      "description": "Build coverage only where needed using spot concealing.",
      "trigger_summary": "lookSpec.base.coverage in ['medium','full'] AND rule_id indicates uneven coverage; localize coverage.",
      "expected_outcomes": [
        "improved correction where needed",
        "avoid heavy all-over base",
        "more natural finish"
      ]
    },
    {
      "intent_id": "INTENT:base_color_correct_before_conceal",
      "area": "base",
      "description": "Use color correction before concealer to reduce discoloration.",
      "trigger_summary": "rule_id indicates under-eye/blemish discoloration mismatch; apply corrector then conceal.",
      "expected_outcomes": [
        "neutralized discoloration",
        "less concealer needed",
        "more natural coverage"
      ]
    },
    {
      "intent_id": "INTENT:base_set_for_longevity",
      "area": "base",
      "description": "Increase wear time and reduce transfer with setting strategy.",
      "trigger_summary": "rule_id indicates breakdown/transfer risk; apply setting powder/spray appropriately.",
      "expected_outcomes": [
        "improved longevity",
        "reduced transfer",
        "finish preserved"
      ]
    },
    {
      "intent_id": "INTENT:eye_liner_angle_adapt",
      "area": "eye",
      "description": "Adapt liner direction/degree to suit user eye geometry while keeping the LookSpec intent.",
      "trigger_summary": "lookSpec.eye.liner_direction.direction in ['up','down'] AND rule_id indicates liner-angle/geometry mismatch; adjust outer-third angle and placement.",
      "expected_outcomes": [
        "less droop or harsh lift",
        "preserve lid space",
        "shape reads closer to reference"
      ]
    },
    {
      "intent_id": "INTENT:eye_liner_degree_soften",
      "area": "eye",
      "description": "Soften liner degree toward straight for subtlety and balance.",
      "trigger_summary": "ABS(lookSpec.eye.liner_direction.degree) is high AND rule_id indicates overemphasis; reduce degree and thickness.",
      "expected_outcomes": [
        "more subtle liner",
        "better symmetry impression",
        "less harsh outer corner"
      ]
    },
    {
      "intent_id": "INTENT:eye_liner_thin_and_tightline",
      "area": "eye",
      "description": "Achieve definition with thin liner plus tightlining rather than a thick wing.",
      "trigger_summary": "rule_id indicates limited lid space OR desire for clean definition; use tightline + thin outer-third line.",
      "expected_outcomes": [
        "defined lashline",
        "more lid space visible",
        "cleaner, lighter look"
      ]
    },
    {
      "intent_id": "INTENT:eye_shadow_shape_adapt",
      "area": "eye",
      "description": "Adjust shadow placement to match the shadow_shape bucket on user anatomy.",
      "trigger_summary": "lookSpec.eye.shadow_shape != 'unknown' AND rule_id indicates shape mismatch; shift placement and blending pattern.",
      "expected_outcomes": [
        "shadow shape matches bucket intent",
        "balanced proportions",
        "crease/outer corner reads correctly"
      ]
    },
    {
      "intent_id": "INTENT:eye_soft_smoky_blend",
      "area": "eye",
      "description": "Create a soft smoky impression with blended edges and controlled depth.",
